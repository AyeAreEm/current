#syslink "c";

extern puts :: fn(s: cstring) i32;

main :: fn() void {
    word := "hello";
    len := word.len;
    ptr := word.ptr;

    words := [_]string{"hello", "world"};

    for (i: usize = 0; i < words.len; i += 1) {
        puts(words[i].ptr);
    }
}
