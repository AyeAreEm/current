extern puts :: fn(s: cstring) i32;

main :: fn() void {
    num := 10;

    cstr := c"hello";
    str := "hello";
    str_ptr := str.ptr;

    greetings := [_]cstring{c"hello", c"hi", c"hey"};
    len := greetings.len;
    greetings[0] = c"wassup";

    for (i: usize = 0; i < greetings.len; i += 1) {
        puts(greetings[i]);
    }
}
