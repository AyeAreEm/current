extern puts :: fn(s: cstring) i32;

Vec2 :: struct {
    x: f32;
    y: f32;
}

Player :: struct {
    pos: Vec2;
    health: u8;
}

player_take_damage :: fn(self: *Player) void {
    puts(c"game over");
}

main :: fn() void {
    player := Player{
        .pos = Vec2{0.0, 0.0},
        .health = 2,
    };

    player_take_damage(&player);
    player_take_damage(&player);
}
