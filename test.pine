extern puts :: fn(s: cstring) i32;

Foo :: enum {
    Bar;
    Baz;
}

main :: fn() void {
    foo := Foo.Bar;

    nums := [_]i32{1, 2, 3, 4, 5};

    switch (foo) {
        fall Foo.Baz:
            puts(c"foo baz");
        case Foo.Bar:
            puts(c"foo bar");
        default:
            puts(c"default");
    }
}
